{
  "name": "kval",
  "version": "0.11.0",
  "description": "The thoughtless datastore. Easy to use, easy to scale.",
  "main": "db.js",
  "scripts": {
    "test": "mocha test/**/*.test.js",
    "debug-test": "DEBUG=kval* mocha test/**/*.test.js",
    "cover": "mocha -r blanket -R html-cov > coverage.html test/**/*.test.js; open coverage.html",
    "bench": "mocha test/**/*.bench.js",
    "local": "DEBUG=kval* nodemon kval.js --webapp=3008 --password=asdf",
    "start": "DEBUG=kval* node kval.js",
    "webapp": "DEBUG=kval* nodemon kval.js",
    "compile": "sudo sh compile",
    "native": "sudo ./build/kval --port=9222 --password=asdf --path=db"
  },
  "config": {
    "blanket": {
      "pattern": [
        ""
      ],
      "data-cover-never": [
        "node_modules",
        "test",
        "kval.js"
      ]
    }
  },
  "author": "Jeff Parrish <jeffhparrish@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "0.10.39"
  },
  "dependencies": {
    "async": "^1.3.0",
    "debug": "^2.2.0",
    "dnode": "^1.2.1",
    "node-lmdb": "^0.3.0"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "jscs": "^1.13.1",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5",
    "nodemon": "^1.3.7",
    "rimraf": "^2.4.1",
    "should": "^7.0.2"
  }
}
