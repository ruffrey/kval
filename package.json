{
  "name": "kval",
  "version": "0.6.0",
  "description": "The thoughtless datastore. Easy to use, easy to scale.",
  "main": "db.js",
  "scripts": {
    "test": "mocha test/**/*.test.js",
    "debug-test": "DEBUG=kval* mocha test/**/*.test.js",
    "cover": "mocha -r blanket -R html-cov > coverage.html test/**/*.test.js; open coverage.html",
    "bench": "mocha test/**/*.bench.js",
    "start": "DEBUG=kval* nodemon kval.js --webapp=3008 --password=asdf",
    "build": "nexe -r 0.10.39 -i index.js -o build/kval -t build/tmp"
  },
  "config": {
    "blanket": {
      "pattern": [
        ""
      ],
      "data-cover-never": [
        "node_modules",
        "test",
        "kval.js"
      ]
    }
  },
  "author": "Jeff Parrish <jeffhparrish@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "0.10.39"
  },
  "dependencies": {
    "body-parser": "^1.13.2",
    "compression": "^1.5.1",
    "connect": "^3.4.0",
    "connect-route": "^0.1.5",
    "debug": "^2.2.0",
    "node-lmdb": "^0.3.0",
    "qs": "^4.0.0",
    "rpc-stream": "^2.1.1",
    "serve-static": "^1.10.0",
    "yargs": "^3.15.0"
  },
  "devDependencies": {
    "async": "^1.3.0",
    "blanket": "^1.1.7",
    "jscs": "^1.13.1",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5",
    "nexe": "^0.4.0",
    "nodemon": "^1.3.7",
    "rimraf": "^2.4.1",
    "should": "^7.0.2"
  }
}
